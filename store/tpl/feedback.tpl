<div class="fb_user">
    <div class="fb_user_firstUnit">
        <div class="name_user">{name}</div>
        <div class="date_birthday_user">
            <div class="text">дата рождения:</div>{date_birthday}
        </div>
    </div>
    <div class="feedback_info_user">
        <div class="text">отзыв:</div>{feedback_info}
    </div>
    <div class="fb_user_secondUnit">
        <div class="rating_user">
            <div class="text">оценка:</div>{rating}
        </div>
        <div class="date_create_user">
            <span class="text">дата отзыва:</span>{date_create}
        </div>
    </div>
</div>
